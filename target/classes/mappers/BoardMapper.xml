<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boardMapper">
	
	<!-- 공지시항 글 작성 -->
	<insert id="insert">

	</insert>
	
	<!-- 공지사항 보기 -->
	<select id="list" resultType="com.mayfarm.freeboard.vo.BoardVO">
		SELECT	Bno as no,
				Btitle as title,
				Bwriter as writer,
				BregDt as regDt,
				BmodDt as modDt,
				Bhit as hit
		FROM myboard
		ORDER BY Bno DESC
	
	</select>
	
</mapper>